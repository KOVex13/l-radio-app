{
  "expo": {
    "name": "«L» радио",
    "slug": "l-radio-app",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0"
          },
          "ios": {
            "deploymentTarget": "13.6"
          }
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location for get audio stream your city or region.",
          "locationAlwaysPermission": "Allow $(PRODUCT_NAME) to use your location for get audio stream your city or region.",
          "locationWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location for get audio stream your city or region."
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/3a076a26-e818-4c7f-a4ca-5b02d166657b"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kovex.lradioapp",
      "infoPlist": {
        "UIBackgroundModes": [
          "audio",
          "fetch"
        ]
      },
      "buildNumber": "18",
      "runtimeVersion": {
        "policy": "sdkVersion"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.kovex.lradioapp",
      "versionCode": 18,
      "runtimeVersion": "1.0.4"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "3a076a26-e818-4c7f-a4ca-5b02d166657b"
      }
    }
  }
}
